#!/bin/bash
# Use 'pgrep' command to find PIDs of processes whose names contain the word "bash"

# Find the PIDs of processes containing the word "bash"
bash_pids=$(pgrep -f bash)

# Get the PID of the current script
script_pid=$$

# Loop through the bash PIDs and display the process name and its PID
for pid in $bash_pids; do
    if [ "$pid" -ne "$script_pid" ]; then
        echo "$pid bash"
    fi
done

